{
  "name": "robot",
  "version": "1.0.0",
  "description": "Search datas automatically",
  "main": "dist/schedules/server.js",
  "scripts": {
    "start": "node dist/schedules/server.js",
    "build": "rimraf dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "dev:server": "ts-node-dev --no-notify --respawn --transpileOnly --ignore-watch node_modules src/schedules/server.ts",
    "dev:queue": "ts-node -r tsconfig-paths/register src/queues/queue.ts | pino-pretty -c -t",
    "dev:goiania": "ts-node -r tsconfig-paths/register src/scrapings/index.ts | pino-pretty -c -t"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.25.0",
    "bull": "^4.5.1",
    "cron": "^1.8.2",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "pino": "^7.9.1",
    "puppeteer": "^13.1.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bull": "^3.15.7",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.15",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "pino-pretty": "^7.5.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "keywords": []
}
